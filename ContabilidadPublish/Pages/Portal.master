        function OnlyNumber() 
        {
            var key = window.event.keyCode;
            
            if (key < 48 || key > 57) 
            {
               if(key == 44)
                window.event.keyCode = 44;
               else
                window.event.keyCode = 0;
            }
            else 
            {                                
               if (key == 13) 
               {
                  window.event.keyCode = 0;
               }
            }
        }
        
        function openWindowSearchEmployee(idUser, vcUserShort, vcUserName) {
            var url;            
            url = "SearchUser.aspx?eidUser=" + idUser + "&evcUserShort=" + vcUserShort + "&evcUserName=" + vcUserName;
            popUpWin(url, 'console', 700, 500);
        }		

		function openProjectCost(IdProyect) 
