        function hiddenControl(ctrl) {

            if (ctrl.style.display == 'none')
                ctrl.style.display = 'block'
            else
                ctrl.style.display = 'none'
        }

        function hiddenMenu(ctrl) {
            if (ctrl) {
                if (ctrl.style.display == 'none') {
                    ctrl.style.display = 'block';
                    //alert('show');
                }
                else {
                    ctrl.style.display = 'none';
                    //alert('hide');
                }
            }
        }
    </script>  
    
